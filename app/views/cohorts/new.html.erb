<%= form_for @cohorts do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :start_date %>
<%= f.text_field :start_date %>
<%= f.label :end_date %>
<%= f.text_field :end_date %>
<h1>Choose Course:</h1>

<%= collection_radio_buttons(:cohort, :course_id, Course.all, :id, :name_with_initial) do |b| %>
<div>
<%= b.radio_button %>
<%= b.label %>
</div>
<% end %>
<%= f.submit %>
<% end %>

<%= datepicker_input form, :end_date%>
<%= datepicker_input form, :start_date%>
